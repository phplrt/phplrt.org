{
    "name": "local/http-factory-bundle",
    "type": "symfony-bundle",
    "require": {
        "php": "^8.2",
        "symfony/yaml": "^5.4|^6.0|^7.0",
        "symfony/http-foundation": "^5.4|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Local\\HttpFactory\\": "src"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.64",
        "phpunit/phpunit": "^10.5|^11.0",
        "vimeo/psalm": "^5.26"
    },
    "autoload-dev": {
        "psr-4": {
            "Local\\HttpFactory\\Tests\\": "tests"
        }
    },
    "suggest": {
        "ext-json": "Adds JSON decoder and encoder",
        "symfony/yaml": "Adds YAML decoder and encoder",
        "rybakit/msgpack": "Adds MSGPACK decoder and encoder",
        "symfony/http-kernel": "Adds Symfony bundle support (Local\\HttpFactory\\HttpFactoryBundle)"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-main": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
