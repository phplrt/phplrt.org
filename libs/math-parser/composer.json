{
    "name": "local/math-parser",
    "require": {
        "php": "^8.1",
        "symfony/var-dumper": "^5.4|^6.0|^7.0",
        "scrivo/highlight.php": "^9.18",
        "phplrt/runtime": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Local\\MathParser\\": "src"
        }
    },
    "require-dev": {
        "phplrt/phplrt": "^3.5",
        "friendsofphp/php-cs-fixer": "^3.49",
        "phpunit/phpunit": "^10.5|^11.0",
        "vimeo/psalm": "^5.21"
    },
    "autoload-dev": {
        "psr-4": {
            "Local\\MathParser\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-main": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
