{
    "name": "local/content-renderer",
    "type": "library",
    "require": {
        "php": "^8.1",
        "ext-simplexml": "*",
        "league/commonmark": "^2.3",
        "scrivo/highlight.php": "^9.18",
        "spatie/commonmark-highlighter": "^3.0",
        "symfony/string": "^5.4|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Local\\ContentRenderer\\": "src"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.49",
        "phpunit/phpunit": "^10.5|^11.0",
        "vimeo/psalm": "^5.21"
    },
    "autoload-dev": {
        "psr-4": {
            "Local\\ContentRenderer\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-main": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
