{
    "name": "local/hydrator-jms",
    "type": "library",
    "require": {
        "php": "^8.2",
        "doctrine/instantiator": "^1.5|^2.0",
        "local/hydrator": "^1.0",
        "jms/serializer": "^1.14|^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Local\\Hydrator\\JMS\\": "src"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.64",
        "phpunit/phpunit": "^10.5|^11.0",
        "vimeo/psalm": "^5.26"
    },
    "autoload-dev": {
        "psr-4": {
            "Local\\Hydrator\\JMS\\Tests\\": "tests"
        }
    },
    "provide": {
        "local/hydrator-implementation": "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-main": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
